#!/usr/bin/env bash

nohup python -u train.py --config configs/config_deepsinv_alt_16const.yml --seed 42 --device cuda:0 > ./logs/deepsinv_alt_16const_s42 &
nohup python -u train.py --config configs/config_deepsinv_alt_16const.yml --seed 121 --device cuda:1 > ./logs/deepsinv_alt_16const_s121 &
nohup python -u train.py --config configs/config_deepsinv_alt_16const.yml --seed 308 --device cuda:2 > ./logs/deepsinv_alt_16const_s308 &
nohup python -u train.py --config configs/config_deepsinv_alt_16const.yml --seed 240 --device cuda:3 > ./logs/deepsinv_alt_16const_s240 &
